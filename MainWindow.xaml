<Window x:Class="CW_HLL2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CW_HLL2"
        mc:Ignorable="d"
        Title="GalagaClone"
        WindowState="Normal"
        WindowStyle="SingleBorderWindow"
        Height="960"
        Width="640" ResizeMode="NoResize" WindowStartupLocation="Manual" MinWidth="640" MinHeight="960" MaxWidth="640" MaxHeight="960"
        >

    <Canvas Name="mCanvas" Focusable="True" Background="Black" KeyDown="KeyIsDown" KeyUp="KeyIsUp">
        <Rectangle Name="Player" Fill="Green" Height="60" Width="60" ></Rectangle>
    </Canvas>
</Window>
